        <activity android:name=".ManagerRegistration" />
        <activity android:name=".StaffRegistration" />
        <activity android:name=".ManagerPanel" />
        <activity android:name=".CustomerPanel" />
        <activity android:name=".StaffPanel" />
        <activity android:name=".CustomerRegistration" />
